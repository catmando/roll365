= form_for @identifier do |f|
  - if @identifier.errors.any?
    #error_explanation
      %h2= "#{pluralize(@identifier.errors.count, "error")} prohibited this identifier from being saved:"
      %ul
        - @identifier.errors.full_messages.each do |message|
          %li= message


  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :value
    = f.text_field :value
  .field
    = f.label :rank
    -#= f.number_field :rank
    = f.select( :rank, %w[1 2 3 4 5 6 7 8 9] )
  .actions
    = f.submit 'Save'
    
