%h3 Search
= search_form_for @q do |f|
  
  
  = f.label :street_address
  = f.text_field :street_address_cont

  = f.label :city
  = f.text_field :city_cont

  = f.label :state
  = f.text_field :state_cont
  
  = f.label :post_code
  = f.text_field :post_code_cont
  
  = f.submit

  = link_to 'Back', addresses_path





